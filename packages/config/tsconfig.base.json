{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "isolatedModules": true,
    "module": "commonjs",
    "target": "es2015",
    "lib": ["dom", "dom.iterable", "ES2018"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "resolveJsonModule": true
  },
  "exclude": ["node_modules"]
}
